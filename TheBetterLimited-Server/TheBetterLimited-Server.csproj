<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>TheBetterLimited_Server</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="6.0.4" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.2.3" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="AppLogic\" />
    <None Remove="AppLogic\controller\" />
    <None Remove="AppLogic\model\" />
    <None Remove="Lib\" />
    <None Remove="Helper\File\" />
    <None Remove="var\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="AppLogic\" />
    <Folder Include="AppLogic\controller\" />
    <Folder Include="AppLogic\model\" />
    <Folder Include="Helper\" />
    <Folder Include="Helper\File\" />
    <Folder Include="var\" />
  </ItemGroup>
  <ItemGroup>
    <None Update="var\EmptyTextFile.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
